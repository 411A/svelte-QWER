{
  "name": "svelte-qwer",
  "version": "0.0.1",
  "scripts": {
    "dev:sveltekit": "MODE=development vite dev --host",
    "dev:QWER": "node --no-warnings QWER watch",
    "dev": "npm-run-all -p -r dev:QWER dev:sveltekit",
    "build:sveltekit": "vite build",
    "build:QWER": "node --no-warnings QWER build",
    "build": "npm-run-all -s build:QWER build:sveltekit",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "clean:sveltekit": "rm .svelte-kit -rf",
    "clean:node_modules": "rm node_modules -rf && cd QWER && rm node_modules -rf",
    "clean": "node --no-warnings QWER clean",
    "deepclean": "npm-run-all -s clean clean:node_modules clean:sveltekit",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "postinstall": "cd QWER && pnpm i",
    "up:deps": "pnpm up -L -i",
    "up:QWER-deps": "cd QWER && pnpm up -L -i",
    "up": "npm-run-all -s up:deps up:QWER-deps"
  },
  "devDependencies": {
    "@giscus/svelte": "^2.1.2",
    "@iconify-json/akar-icons": "^1.1.8",
    "@iconify-json/carbon": "^1.1.7",
    "@iconify-json/ic": "^1.1.8",
    "@iconify-json/mdi": "^1.1.29",
    "@iconify-json/tabler": "^1.1.21",
    "@sveltejs/adapter-auto": "1.0.0-next.64",
    "@sveltejs/kit": "1.0.0-next.396",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@unocss/core": "^0.44.7",
    "@unocss/preset-icons": "^0.44.7",
    "@unocss/preset-uno": "^0.44.7",
    "@unocss/reset": "^0.44.7",
    "@unocss/transformer-compile-class": "^0.44.7",
    "@unocss/transformer-directives": "^0.44.7",
    "@unocss/transformer-variant-group": "^0.44.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.54.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "unocss": "^0.44.7",
    "vite": "^3.0.3"
  },
  "type": "module",
  "peerDependencies": {
    "vite": "^2.9.13"
  }
}
