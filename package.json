{
  "name": "@kwchang0831/svelte-qwer",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "repository": "kwchang0831/svelte-qwer",
  "homepage": "https://github.com/kwchang0831/svelte-qwer",
  "bugs": "https://github.com/kwchang0831/svelte-qwer/issues",
  "author": "kwchang0831",
  "scripts": {
    "dev:sveltekit": "MODE=development vite dev",
    "dev:QWER": "node --no-warnings QWER watch",
    "dev": "npm-run-all -p -r dev:QWER dev:sveltekit",
    "build:sveltekit": "vite build",
    "build:QWER": "node --no-warnings QWER build",
    "build": "npm-run-all -s build:QWER build:sveltekit",
    "preview": "vite preview",
    "clean:sveltekit": "rm .svelte-kit -rf",
    "clean:node_modules": "rm node_modules -rf && cd QWER && rm node_modules -rf",
    "clean": "node --no-warnings QWER clean",
    "deepclean": "npm-run-all -s clean clean:node_modules clean:sveltekit",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "postinstall": "cd QWER && pnpm i",
    "up:deps": "pnpm up -L -i",
    "up:QWER-deps": "cd QWER && pnpm up -L -i",
    "up": "npm-run-all -s up:deps up:QWER-deps"
  },
  "devDependencies": {
    "@iconify-json/akar-icons": "^1.1.8",
    "@iconify-json/carbon": "^1.1.7",
    "@iconify-json/ic": "^1.1.8",
    "@iconify-json/line-md": "^1.1.13",
    "@iconify-json/mdi": "^1.1.29",
    "@iconify-json/tabler": "^1.1.21",
    "@sveltejs/adapter-netlify": "1.0.0-next.71",
    "@sveltejs/adapter-node": "1.0.0-next.85",
    "@sveltejs/adapter-static": "1.0.0-next.38",
    "@sveltejs/adapter-vercel": "1.0.0-next.66",
    "@sveltejs/kit": "1.0.0-next.403",
    "@types/lz-string": "^1.3.34",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@unocss/core": "^0.45.5",
    "@unocss/preset-icons": "^0.45.5",
    "@unocss/preset-uno": "^0.45.5",
    "@unocss/reset": "^0.45.5",
    "@unocss/transformer-compile-class": "^0.45.5",
    "@unocss/transformer-directives": "^0.45.5",
    "@unocss/transformer-variant-group": "^0.45.5",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "lz-string": "^1.4.4",
    "medium-zoom": "^1.0.6",
    "npm-run-all": "^4.1.5",
    "plyr": "^3.7.2",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.54.3",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tippy.js": "^6.3.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "unocss": "^0.45.5",
    "unocss-preset-scrollbar": "^0.2.1",
    "vite": "^3.0.4",
    "vite-imagetools": "^4.0.4"
  }
}
