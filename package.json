{
  "name": "svelte-qwer",
  "version": "0.0.1",
  "scripts": {
    "dev:sveltekit": "MODE=development vite dev --host",
    "dev:QWER": "node --no-warnings QWER watch",
    "dev": "npm-run-all -p -r dev:QWER dev:sveltekit",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "clean": "node --no-warnings QWER clean",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@giscus/svelte": "^2.0.6",
    "@iconify-json/akar-icons": "^1.1.7",
    "@iconify-json/carbon": "^1.1.6",
    "@iconify-json/ic": "^1.1.7",
    "@iconify-json/mdi": "^1.1.29",
    "@iconify-json/tabler": "^1.1.20",
    "@sveltejs/adapter-auto": "1.0.0-next.63",
    "@sveltejs/kit": "1.0.0-next.390",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@unocss/core": "^0.44.5",
    "@unocss/preset-icons": "^0.44.5",
    "@unocss/preset-uno": "^0.44.5",
    "@unocss/reset": "^0.44.5",
    "@unocss/transformer-compile-class": "^0.44.5",
    "@unocss/transformer-directives": "^0.44.5",
    "@unocss/transformer-variant-group": "^0.44.5",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.53.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "unocss": "^0.44.5",
    "vite": "^3.0.2"
  },
  "type": "module",
  "peerDependencies": {
    "vite": "^2.9.13"
  }
}
